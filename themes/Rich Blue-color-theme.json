    "editorGroup.emptyBackground": "#191720",
    "editorGroupHeader.noTabsBackground": "#191720",
    "editorGroupHeader.tabsBackground": "#1a1b26",
    "editorGroupHeader.tabsBorder": "#7aa2f744",
    "tab.activeBackground": "#191720",
    "tab.activeBorder": "#191720",
