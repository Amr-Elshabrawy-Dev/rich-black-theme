{
  "$schema": "vscode://schemas/color-theme",
  "name": "Rich Blue",
  "type": "dark",
  "semanticHighlighting": true,
  "colors": {
    // Contrast Colors - Colors determining the contrast ratio of UI elements.
    "contrastBorder": "#292e42",
    // Base Colors - Basic colors used throughout the UI.
    "focusBorder": "#292e42",
    "foreground": "#c0caf5",
    "widget.shadow": "#191720",
    "selection.background": "#7aa2f7",
    "errorForeground": "#f7768e",
    // Window Border - The theme colors for VS Code window border.
    "window.activeBorder": "#7aa2f7",
    "window.inactiveBorder": "#191720",
    // Text Colors - Colors for text elements such as descriptions or labels.
    "textCodeBlock.background": "#292e42",
    // Button Control - A set of colors for button widgets such as Open Folder button in the Explorer of a new window.
    "button.background": "#7aa2f780",
    "button.foreground": "#ffffff",
    "button.hoverBackground": "#7aa2f7cc",
    // Dropdown Control - A set of colors for all dropdown widgets such as in the Integrated Terminal or the Output panel.
    "dropdown.background": "#191720",
    "dropdown.border": "#7aa2f780",
    "dropdown.foreground": "#ffffffcc",
    // Input Control - Colors for input controls such as in the Search view or the Find/Replace dialog.
    "input.background": "#1a1b26",
    "input.border": "#7aa2f7",
    "input.foreground": "#c0caf5",
    "input.placeholderForeground": "#7aa2f780",
    "inputOption.activeBorder": "#c0caf5",
    "inputValidation.errorBackground": "#f7768ef2",
    "inputValidation.errorBorder": "#f7768e",
    "inputValidation.infoBackground": "#7aa2f7f2",
    "inputValidation.infoBorder": "#7aa2f7",
    "inputValidation.warningBackground": "#e0af68f2",
    "inputValidation.warningBorder": "#e0af68",
    // Scrollbar Control - Colors for the scroll bar control.
