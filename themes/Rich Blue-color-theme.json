    "editorRuler.foreground": "#7aa2f760",
    "editorWarning.foreground": "#e0af68",
    "editorWhitespace.foreground": "#ffffff1a",
    // Editor Widget Colors - The Editor widget is shown in front of the editor content. Examples are the Find/Replace dialog, the suggestion widget, and the editor hover.
    "editorWidget.background": "#1a1b26",
    "editorWidget.border": "#7aa2f7",
    "editorWidget.resizeBorder": "#7aa2f7",
    "editorSuggestWidget.background": "#1a1b26",
    "editorSuggestWidget.border": "#7aa2f7",
    "editorSuggestWidget.foreground": "#c0caf5",
    "editorSuggestWidget.highlightForeground": "#ffffff",
    "editorSuggestWidget.selectedBackground": "#7aa2f7cc",
    "editorSuggestWidget.selectedIconForeground": "#ffffff",
    "editorHoverWidget.background": "#1a1b26",
    "editorHoverWidget.border": "#7aa2f7",
    // Peek View Colors - Peek views are used to show references and declarations as results of go to definition, peek definition and find references.
    "peekView.border": "#7aa2f7",
    "peekViewEditor.background": "#191720",
    "peekViewEditor.matchHighlightBackground": "#bb9af75a",
    "peekViewResult.background": "#191720",
    "peekViewResult.fileForeground": "#7aa2f7",
    "peekViewResult.lineForeground": "#7aa2f7",
    "peekViewResult.matchHighlightBackground": "#ffffffcc",
    "peekViewResult.selectionBackground": "#292e42",
    "peekViewResult.selectionForeground": "#7aa2f7",
    "peekViewTitle.background": "#191720",
    "peekViewTitleDescription.foreground": "#565f89",
    "peekViewTitleLabel.foreground": "#7aa2f7",
    // Merge Conflicts Colors - Colors for merge conflicts.
    "merge.border": "#7aa2f7",
    "merge.currentContentBackground": "#7aa2f733",
    "merge.currentHeaderBackground": "#7aa2f766",
    "merge.incomingContentBackground": "#bb9af75a",
    "merge.incomingHeaderBackground": "#bb9af75a",
    // Panel Colors - Panels are shown below the editor area and contain views like Output and Integrated Terminal.
    "panel.background": "#1a1b26",
    "panel.border": "#7aa2f780",
    "panelTitle.activeBorder": "#7aa2f7",
    "panelTitle.activeForeground": "#c0caf5",
    "panelTitle.inactiveForeground": "#c0caf580",
    // Status Bar Colors - The Status Bar is shown in the bottom of the workbench.
    "statusBar.background": "#1a1b26",
    "statusBar.border": "#7aa2f780",
    "statusBar.debuggingBackground": "#191720",
    "statusBar.debuggingForeground": "#c0caf5",
    "statusBar.debuggingBorder": "#292e42",
    "statusBar.foreground": "#c0caf5",
    "statusBar.noFolderBackground": "#191720",
    "statusBar.noFolderForeground": "#7aa2f7",
    "statusBar.noFolderBorder": "#292e42",
    "statusBarItem.activeBackground": "#191720",
    "statusBarItem.hoverBackground": "#191720",
    "statusBarItem.prominentBackground": "#191720",
    "statusBarItem.prominentHoverBackground": "#191720",
    "statusBarItem.remoteBackground": "#7aa2f7",
    "statusBarItem.remoteForeground": "#ffffff",
    // Title Bar Colors - Colors for the Title Bar.
    "titleBar.activeBackground": "#1a1b26",
    "titleBar.activeForeground": "#c0caf5",
    "titleBar.border": "#7aa2f780",
    "titleBar.inactiveBackground": "#191720",
    "titleBar.inactiveForeground": "#c0caf5",
    // Menu Bar Colors - Colors for the Menu Bar.
    "menu.background": "#1a1b26",
    "menu.border": "#7aa2f780",
    // Notification Colors - Colors for Notifications. Notifications slide in from the top of the workbench.
    "notificationCenter.border": "#7aa2f7",
    "notificationLink.foreground": "#7dcfff",
    "notificationToast.border": "#7aa2f7",
    "notifications.background": "#1a1b26",
    "notifications.border": "#7aa2f7",
    "notifications.foreground": "#ffffffcc",
    // Extensions Colors - Colors for the Extensions view.
    "extensionButton.prominentBackground": "#7aa2f7cc",
    "extensionButton.prominentForeground": "#c0caf5",
    "extensionButton.prominentHoverBackground": "#7aa2f7",
    // Quick Picker Colors - Colors for the Quick Picker. The Quick Picker is the container for pickers like the Command Palette.
    "pickerGroup.border": "#191720",
    "pickerGroup.foreground": "#bb9af7",
    // Integrated Terminal Colors - Colors for the Integrated Terminal.
    "terminal.ansiBlack": "#1a1b26",
    "terminal.ansiBlue": "#7aa2f7",
    "terminal.ansiBrightBlack": "#565f89",
    "terminal.ansiBrightBlue": "#7aa2f7",
    "terminal.ansiBrightCyan": "#7dcfff",
    "terminal.ansiBrightGreen": "#9ece6a",
    "terminal.ansiBrightMagenta": "#bb9af7",
    "terminal.ansiBrightRed": "#ff9e64",
    "terminal.ansiBrightWhite": "#ffffff",
    "terminal.ansiBrightYellow": "#e0af68",
    "terminal.ansiCyan": "#7dcfff",
    "terminal.ansiGreen": "#9ece6a",
    "terminal.ansiMagenta": "#bb9af7",
    "terminal.ansiRed": "#f7768e",
    "terminal.ansiWhite": "#ffffff",
    "terminal.ansiYellow": "#e0af68",
    "terminal.background": "#1a1b26",
    "terminal.foreground": "#c0caf5",
    "terminal.selectionBackground": "#565f89",
    "terminalCursor.background": "#c0caf5",
    "terminalCursor.foreground": "#c0caf5",
    // Debug Colors - Colors for the Debug view.
    "debugExceptionWidget.background": "#191720",
    "debugExceptionWidget.border": "#7aa2f7",
    "debugToolBar.background": "#191720",
    "debugTokenExpression.boolean": "#bb9af7",
    "debugTokenExpression.name": "#7aa2f7",
    "debugTokenExpression.number": "#ff9e64",
    "debugTokenExpression.string": "#e0af68",
    "debugTokenExpression.value": "#9ece6a",
    // Welcome Page Colors - Colors for the Welcome page.
    "walkThrough.embeddedEditorBackground": "#191720",
    // Git Decoration Colors - Colors for Git decorations.
    "gitDecoration.addedResourceForeground": "#bb9af7",
    "gitDecoration.conflictingResourceForeground": "#e0af68cc",
    "gitDecoration.deletedResourceForeground": "#f7768e90",
    "gitDecoration.ignoredResourceForeground": "#565f89",
    "gitDecoration.modifiedResourceForeground": "#7aa2f7",
    "gitDecoration.stageModifiedResourceForeground": "#7aa2f7",
    "gitDecoration.submoduleResourceForeground": "#bb9af7",
    "gitDecoration.untrackedResourceForeground": "#9ece6aff",
    // Settings Editor Colors - Colors for the Settings Editor.
    "settings.headerForeground": "#7aa2f7",
    // Breadcrumbs Colors - Colors for Breadcrumbs.
    "breadcrumb.activeSelectionForeground": "#c0caf5",
    "breadcrumb.background": "#191720",
    "breadcrumb.focusForeground": "#ffffff",
    "breadcrumb.foreground": "#bb9af7",
    "breadcrumbPicker.background": "#191720",
    // Symbol Icons Colors - Colors for Symbol Icons.
    "icon.foreground": "#c0caf5",
    // Command Center Colors - Colors for the Command Center.
    "commandCenter.activeBackground":"#1a1b26",
    "commandCenter.activeBorder":"#7aa2f7cc",
    "commandCenter.activeForeground": "#c0caf5",
    "commandCenter.background": "#191720",
    "commandCenter.border":"#7aa2f780",
    "commandCenter.foreground": "#c0caf5cc",
    "commandCenter.inactiveBorder": "#7aa2f780",
  },
  "semanticTokenColors": {
    "class.declaration": {
      "foreground": "#f7768e" // Matches other class-related colors
    },
    "class.decorator": {
      "foreground": "#9ece6a" // Matches decorator colors
    },
    "enumMember": {
      "foreground": "#7dcfff" // Matches constant/enum colors
    },
    "meta.decorator": {
      "foreground": "#9ece6a" // Consistent with decorator colors
    },
    "namespace": {
      "foreground": "#ff9e64" // Matches namespace/module colors
    },
    "parameter": {
      "foreground": "#f7768e" // Matches parameter colors
    },
    "property": {
      "foreground": "#7dcfff" // Matches property colors
    },
    "property.declaration": {
      "foreground": "#f7768e" // Matches base text color
    },
    "variable": {
      "foreground": "#c0caf5" // Matches variable colors
    },
    "variable.defaultLibrary": {
      "foreground": "#BB9AF7" // Matches built-in/default library colors
    }
  },
  "tokenColors": [
    // Consolidated base font style rule
    {
      "scope": [
        "comment",
        "string",
        "variable.parameter.function",
        "entity.name.tag",
        "entity.name.type.class",
        "variable",
        "keyword",
        "keywords",
        "storage.modifier",
        "storage.type",
        "support.class.builtin",
        "keyword.control",
        "constant",
        "constant.language",
        "entity.other.attribute-name",
        "entity.name.method",
        "support.type.property-name.json",
        "support.function",
        "support.type.property-name",
        "support.constant.media.css",
        "text.html.derivative",
        "entity.name.function"
      ],
      "settings": {
        "fontStyle": "italic bold"
      }
    },
    {
      "scope": [
        "entity.name.class",
        "variable.other.enummember"
      ],
      "settings": {
        "foreground": "#f7768e"
      }
    },
    {
      "scope": "variable.other.readwrite",
      "settings": {
        "foreground": "#c0caf5" // Light blue for variables
      }
    },
    {
      "scope": "support.variable",
      "settings": {
        "foreground": "#bb9af7" // Keep violet for support variables
      }
    },
    // Merged cyan operator styles
    {
      "scope": [
        "invalid",
        "keyword.operator",
        "constant.numeric.css",
        "constant.numeric.decimal",
        "constant.numeric.json",
        "punctuation.definition.template-expression",
        "keyword.other.unit.px.css",
        "keyword.other.unit"
      ],
      "settings": {
        "fontStyle": "italic bold",
        "foreground": "#7dcfff"
      }
    },
    {
      "scope": [
        "support.constant.property-value.css",
      ],
      "settings": {
        "fontStyle": "italic bold",
        "foreground": "#F7768E" // Keep red
      }
    },
    {
      "scope": [
        "support.type.property-name.css",
        "punctuation.definition.tag"
      ],
      "settings": {
        "fontStyle": "italic bold",
        "foreground": "#BB9AF7" // Keep violet
      }
    },
    {
      "scope": [
        "source.css variable.parameter.keyframe-list.css",
        "entity.other.keyframe-offset.percentage.css"
      ],
      "settings": {
        "fontStyle": "italic bold",
        "foreground": "#7aa2f7" // Keep blue
      }
    },
    {
      "scope": [
        "punctuation.separator",
        "punctuation.terminator",
        "punctuation.accessor"
      ],
      "settings": {
        "fontStyle": "italic bold",
        "foreground": "#c0caf5" // Light blue for punctuation
      }
    },
    {
      "scope": [
        "support.module.elm",
        "entity.other.inherited-class.python",
        "variable.other.object",
        "support.other.namespace.php",
        "entity.other.attribute-name.namespace.xml",
        "record.accessor",
        "entity.name.record.field.accessor",
        "storage.modifier.import.java",
        "storage.modifier.import.groovy",
        "meta.import.swift entity.name.type.swift",
        "meta.member.access.python",
        "variable.other.property",
        "variable.other.property.ts",
        "variable.other.property.js",
        "constant.language.symbol.elixir",
        "punctuation.tag.liquid support.class.liquid",
        "meta.scriptblock.powershell meta.group.simple.subexpression.powershell meta.group.simple.subexpression.powershell meta.scriptblock.powershell meta.scriptblock.powershell support.variable.automatic.powershell punctuation.definition.variable.powershell",
        "meta.scriptblock.powershell meta.group.simple.subexpression.powershell meta.group.simple.subexpression.powershell meta.scriptblock.powershell meta.scriptblock.powershell support.variable.automatic.powershell",
        "source.cpp meta.block.class.cpp meta.body.class.cpp",
        "source.cpp meta.block.class.cpp meta.body.class.cpp meta.function.definition.special.constructor.cpp meta.body.function.definition.special.constructor.cpp"
      ],
      "settings": {
        "foreground": "#ff9e64" // Orange for objects/properties
      }
    },
    {
      "scope": [
        "entity.name.type"
      ],
      "settings": {
        "foreground": "#9ece6a" // Keep green
      }
    },
    {
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "string.quoted.docstring.multi",
        "comment.block.documentation source"
      ],
      "settings": {
        "fontStyle": "italic bold",
        "foreground": "#565f89" // Dark blue for comments
      }
    },
    {
      "scope": [
        "constant",
        "support.constant.core.php"
      ],
      "settings": {
        "foreground": "#f7768e" // Keep red
      }
    },
    {
      "scope": [
        "meta.section.struct.go variable.other.field.go",
        "meta.section.struct.go variable.other.declaration.go",
        "source",
        "support",
        "constant.character.entity.named",
        "meta.jsx.children",
        "source.lua",
        "text.html.derivative",
        "source.graphql meta.selections variable.other.alias.graphql",
        "source.graphql meta.type.interface.graphql meta.type.object.graphql variable.graphql",
        "source.graphql variable.arguments.graphql",
        "JSXNested"
      ],
      "settings": {
        "foreground": "#c0caf5" // Light blue
      }
    },
    {
      "scope": [
        "entity.name.function.decorator",
        "meta.decorator entity.name.function",
        "meta.decorator punctuation.decorator",
        "punctuation.definition.decorator",
        "meta.decorator variable.other.readwrite.ts",
        "variable.parameter.gdscript",
        "entity.name.variable.parameter",
        "variable.parameter",
        "meta.decorator"
      ],
      "settings": {
        "foreground": "#bb9af7" // Violet for decorators
      }
    },
    {
      "scope": [
        "keyword.control.export",
        "support.type.object.module"
      ],
      "settings": {
        "foreground": "#7aa2f7" // Blue for exports/modules
      }
    },
    {
      "scope": [
        "support.function",
        "entity.name.function",
        "entity.name.function.js",
        "entity.name.method",
        "meta.function-call",
        "meta.function",
        "meta.method.declaration",
        "meta.function-call support",
        "variable.language.super.ts",
        "source.directive",
        "meta.function-call.generic",
        "meta.method-call.static.php",
        "meta.method-call.php",
        "meta.class storage.type",
        "meta.method.groovy",
        "meta.bracket.square.access",
        "entity.name.function-call.elixir",
        "punctuation.output.liquid support.variable.liquid",
        "meta.function.echo.edge source.js keyword.operator.error-control.js",
        "entity.name.type.variant.gdscript",
        "entity.name.function.powershell"
      ],
      "settings": {
        "foreground": "#7dcfff" // Cyan for functions
      }
    },
    {
      "scope": [
        "keyword.control.import",
        "meta.module.name.elm support",
        "meta.import.elm support",
        "keyword.control.at-rule",
        "punctuation.definition.keyword",
        "variable.control.import.include",
        "keyword.other.import.java"
      ],
      "settings": {
        "foreground": "#9ece6a" // Green for imports
      }
    },
    {
      "scope": [
        "keyword.package.go",
        "keyword.operator.pointer.go",
        "keyword.control",
        "keyword.control.conditional",
        "storage.modifier",
        "keyword.operator.new",
        "keyword.operator.expression",
        "keyword.begin.blade",
        "keyword.end.blade",
        "keyword.blade",
        "keyword.type-alias",
        "keyword.type",
        "support.class.console",
        "keyword.other.await",
        "storage.type.class.jsdoc",
        "punctuation.definition.block.tag.jsdoc",
        "keyword.other.new",
        "keyword.other.phpdoc",
        "keyword.other.use.php",
        "keyword.operator.logical.python",
        "keyword.other.import.static.groovy",
        "keyword.other.import.groovy",
        "punctuation.definition.attribute.swift",
        "source.svelte meta.scope.tag.main.svelte meta.tag.start.svelte variable.function.svelte",
        "meta.directive.on.svelte entity.name.type.svelte",
        "keyword.other.special-method.elixir",
        "keyword.edge",
        "entity.name.function.edge",
        "support.constant.edge",
        "keyword.other.gdscript",
        "keyword.other.onready.gdscript",
        "source.graphql meta.fragment.graphql keyword.on.graphql",
        "source.graphql meta.type.interface.graphql keyword.implements.graphql",
        "source.graphql meta.type.interface.graphql meta.type.object.graphql meta.type.list.graphql keyword.operator.nulltype.graphql",
        "source.graphql meta.variables.graphql keyword.operator.nulltype.graphql",
        "source.graphql meta.type.interface.graphql meta.type.object.graphql keyword.operator.nulltype.graphql",
        "keyword.operator.class.php",
        "keyword.operator.spread.php",
