    "editor.background": "#191720",
    "editor.findMatchBackground": "#7aa2f744",
    "editor.findMatchHighlightBackground": "#7dcfff22",
    "editor.findMatchHighlightBorder": "#ffffff33",
    "editor.findRangeHighlightBackground": "#7dcfff40",
