    "tab.activeForeground": "#c0caf5",
    "tab.border": "#7aa2f780",
    "tab.hoverBackground": "#7aa2f740",
    "tab.hoverForeground": "#c0caf5",
    "tab.inactiveBackground": "#1a1b26",
    "tab.inactiveForeground": "#c0caf5cc",
    "tab.lastPinnedBorder": "#1a1b26",
    "tab.unfocusedActiveBorder": "#1a1b26",
    "tab.unfocusedActiveForeground": "#7aa2f7",
    "tab.unfocusedInactiveForeground": "#7aa2f7",
