    "scrollbarSlider.activeBackground": "#7aa2f780",
    "scrollbarSlider.background": "#7aa2f780",
    "scrollbarSlider.hoverBackground": "#7aa2f780",
    // Badge - Badges are small information labels, e.g. for search results count.
    "badge.background": "#7aa2f7",
    "badge.foreground": "#ffffff",
    // Progress Bar - Colors for the progress bar widget.
    "progressBar.background": "#bb9af7",
    // Lists and Trees - Colors for list and trees like the File Explorer. An active list/tree has keyboard focus, an inactive does not.
    "list.activeSelectionBackground": "#7aa2f780",
    "list.activeSelectionForeground": "#ffffff",
    "list.dropBackground": "#191720",
    "list.errorForeground": "#f7768e",
    "list.focusBackground": "#7aa2f740",
