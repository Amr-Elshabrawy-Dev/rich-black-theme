    "list.hoverBackground": "#7aa2f740",
    "list.hoverForeground": "#ffffff",
    "list.inactiveSelectionBackground": "#7aa2f780",
    "list.inactiveSelectionForeground": "#ffffff",
    "list.invalidItemForeground": "#f7768e",
    "list.warningForeground": "#e0af68",
    "tree.indentGuidesStroke": "#7aa2f780",
    // Activity Bar - The Activity Bar is displayed either on the far left or right of the workbench and allows fast switching between views of the Side Bar.
    "activityBar.background": "#1a1b26",
    "activityBar.activeBackground": "#7aa2f740",
    "activityBar.activeBorder": "#7aa2f7",
    "activityBar.activeFocusBorder": "#7aa2f7",
    "activityBar.border": "#7aa2f780",
    "activityBar.foreground": "#7aa2f7",
    "activityBarBadge.background": "#7aa2f7",
    "activityBarBadge.foreground": "#ffffff",
