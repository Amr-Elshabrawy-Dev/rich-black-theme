    "sideBar.border": "#7aa2f780",
    "sideBar.foreground": "#c0caf5",
    "sideBarSectionHeader.background": "#191720",
    "sideBarSectionHeader.foreground": "#7aa2f7",
    "sideBarTitle.foreground": "#7aa2f7",
    // Minimap - The Minimap shows a minified version of the current file.
    "minimap.findMatchHighlight": "#7aa2f780",
